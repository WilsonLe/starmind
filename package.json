{
  "name": "starmind",
  "version": "1.0.0",
  "description": "REST API with that acts as LLM Tools for AI Agents",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "format": "prettier --write src",
    "generate:openapi": "tsx src/generate-openapi.ts",
    "package": "tar -czvf starmind.tar.gz .next/standalone"
  },
  "dependencies": {
    "@payloadcms/db-sqlite": "3.28.1",
    "@payloadcms/email-resend": "3.28.1",
    "@payloadcms/next": "3.28.1",
    "@payloadcms/payload-cloud": "3.28.1",
    "@payloadcms/richtext-lexical": "3.28.1",
    "cross-env": "7.0.3",
    "graphql": "16.10.0",
    "next": "15.2.2",
    "payload": "3.28.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@openapi-contrib/json-schema-to-openapi-schema": "3.0.3",
    "@types/lodash.mergewith": "4.6.9",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "9.22.0",
    "eslint-config-next": "15.2.2",
    "lodash.mergewith": "4.6.2",
    "openapi-types": "12.1.3",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "ts-essentials": "10.0.4",
    "tsx": "4.19.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
